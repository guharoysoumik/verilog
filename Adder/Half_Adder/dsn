#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x15861b0 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x15b9ef0_0 .net "cout", 0 0, L_0x15ba1d0; 1 drivers
v0x15b9fc0_0 .var "myData", 1 0;
v0x15ba040_0 .net "sum", 0 0, L_0x15ba0f0; 1 drivers
L_0x15ba320 .part v0x15b9fc0_0, 1, 1;
L_0x15ba3c0 .part v0x15b9fc0_0, 0, 1;
S_0x15aabf0 .scope module, "ha" "myHalfAdder" 2 4, 3 1, S_0x15861b0;
 .timescale 0 0;
L_0x15ba0f0 .functor XOR 1, L_0x15ba320, L_0x15ba3c0, C4<0>, C4<0>;
L_0x15ba1d0 .functor AND 1, L_0x15ba320, L_0x15ba3c0, C4<1>, C4<1>;
v0x15aace0_0 .net "A", 0 0, L_0x15ba320; 1 drivers
v0x15b9d00_0 .net "B", 0 0, L_0x15ba3c0; 1 drivers
v0x15b9da0_0 .alias "cout", 0 0, v0x15b9ef0_0;
v0x15b9e40_0 .alias "sum", 0 0, v0x15ba040_0;
    .scope S_0x15861b0;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "myoutput.vcd";
    %vpi_call 2 7 "$dumpvars", 1'sb0, S_0x15861b0;
    %set/v v0x15b9fc0_0, 0, 2;
T_0.0 ;
    %load/v 8, v0x15b9fc0_0, 2;
    %cmpi/u 8, 3, 2;
    %inv 4, 1;
    %jmp/0xz T_0.1, 4;
    %delay 2, 0;
    %load/v 8, v0x15b9fc0_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %ix/load 0, 2, 0;
    %assign/v0 v0x15b9fc0_0, 0, 8;
    %vpi_call 2 12 "$monitor", "%h", v0x15b9fc0_0;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x15861b0;
T_1 ;
    %delay 500, 0;
    %vpi_call 2 18 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "myHalfAdder.v";
