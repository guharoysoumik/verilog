#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x272ec00 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x273e9f0_0 .var "clk", 0 0;
v0x273eac0_0 .var "reset", 0 0;
v0x273eb70_0 .net "value", 7 0, v0x273e8b0_0; 1 drivers
S_0x272ecf0 .scope module, "c" "counter" 2 24, 3 1, S_0x272ec00;
 .timescale 0 0;
P_0x272dd28 .param/l "WIDTH" 3 3, +C4<01000>;
v0x270ba80_0 .net "clk", 0 0, v0x273e9f0_0; 1 drivers
v0x273e8b0_0 .var "out", 7 0;
v0x273e950_0 .net "reset", 0 0, v0x273eac0_0; 1 drivers
E_0x272f490 .event edge, v0x273e950_0;
E_0x272f3a0 .event posedge, v0x270ba80_0;
    .scope S_0x272ecf0;
T_0 ;
    %wait E_0x272f3a0;
    %load/v 8, v0x273e8b0_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x273e8b0_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x272ecf0;
T_1 ;
    %wait E_0x272f490;
    %load/v 8, v0x273e950_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.0, 4;
    %cassign/v v0x273e8b0_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x273e8b0_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x272ec00;
T_2 ;
    %set/v v0x273eac0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x272ec00;
T_3 ;
    %vpi_call 2 5 "$dumpfile", "myoutput.vcd";
    %vpi_call 2 6 "$dumpvars", 1'sb0, S_0x272ec00;
    %delay 15, 0;
    %set/v v0x273eac0_0, 1, 1;
    %delay 10, 0;
    %set/v v0x273eac0_0, 0, 1;
    %delay 500, 0;
    %vpi_call 2 14 "$finish";
    %end;
    .thread T_3;
    .scope S_0x272ec00;
T_4 ;
    %set/v v0x273e9f0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x272ec00;
T_5 ;
    %delay 1, 0;
    %load/v 8, v0x273e9f0_0, 1;
    %inv 8, 1;
    %set/v v0x273e9f0_0, 8, 1;
    %jmp T_5;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "counter.v";
