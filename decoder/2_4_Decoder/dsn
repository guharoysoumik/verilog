v0x1068a20_0 .var "myData", 2 0;
v0x1068ae0_0 .net "o0", 0 0, v0x1068750_0; 1 drivers
v0x1068b60_0 .net "o1", 0 0, v0x1068800_0; 1 drivers
v0x1068c10_0 .net "o2", 0 0, v0x10688a0_0; 1 drivers
v0x1068cf0_0 .net "o3", 0 0, v0x1068980_0; 1 drivers
L_0x1068da0 .part v0x1068a20_0, 2, 1;
L_0x1068ea0 .part v0x1068a20_0, 1, 1;
L_0x1068fc0 .part v0x1068a20_0, 0, 1;
S_0x10336c0 .scope module, "dcode_2_4" "decoder_2_4" 2 3, 3 1, S_0x10339c0;
 .timescale 0 0;
v0x1033d00_0 .net "e", 0 0, L_0x1068fc0; 1 drivers
v0x1068610_0 .net "i0", 0 0, L_0x1068ea0; 1 drivers
v0x10686b0_0 .net "i1", 0 0, L_0x1068da0; 1 drivers
v0x1068750_0 .var "y0", 0 0;
v0x1068800_0 .var "y1", 0 0;
v0x10688a0_0 .var "y2", 0 0;
v0x1068980_0 .var "y3", 0 0;
E_0x10337e0 .event edge, v0x1033d00_0, v0x1068610_0, v0x10686b0_0;
    .scope S_0x10336c0;
T_0 ;
    %wait E_0x10337e0;
    %set/v v0x1068750_0, 0, 1;
    %set/v v0x1068800_0, 0, 1;
    %set/v v0x10688a0_0, 0, 1;
    %set/v v0x1068980_0, 0, 1;
    %load/v 8, v0x1033d00_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x1068610_0, 1;
    %load/v 9, v0x10686b0_0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.4, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.5, 6;
    %jmp T_0.6;
T_0.2 ;
    %set/v v0x1068750_0, 1, 1;
    %jmp T_0.6;
T_0.3 ;
    %set/v v0x1068800_0, 1, 1;
    %jmp T_0.6;
T_0.4 ;
    %set/v v0x10688a0_0, 1, 1;
    %jmp T_0.6;
T_0.5 ;
    %set/v v0x1068980_0, 1, 1;
    %jmp T_0.6;
T_0.6 ;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x10339c0;
T_1 ;
    %set/v v0x1068a20_0, 0, 3;
    %vpi_call 2 7 "$dumpfile", "decoder_2_4_wave.vcd";
    %vpi_call 2 8 "$dumpvars", 1'sb0, S_0x10339c0;
    %delay 500, 0;
    %vpi_call 2 9 "$finish";
    %end;
    .thread T_1;
    .scope S_0x10339c0;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x1068a20_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %set/v v0x1068a20_0, 8, 3;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
