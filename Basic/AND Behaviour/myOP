    .port_info 2 /OUTPUT 1 "y";
v0x6407894e3cf0_0 .net "a", 0 0, L_0x64078952f1d0;  1 drivers
v0x64078952ee20_0 .net "b", 0 0, L_0x64078952f2d0;  1 drivers
v0x64078952eee0_0 .var "y", 0 0;
E_0x6407894e4480 .event edge, v0x64078952ee20_0, v0x6407894e3cf0_0;
    .scope S_0x64078951e120;
T_0 ;
    %end;
    .thread T_0;
    .scope S_0x64078951e120;
T_1 ;
    %wait E_0x6407894e4480;
    %load/vec4 v0x6407894e3cf0_0;
    %cmpi/e 0, 0, 1;
    %flag_mov 8, 4;
    %load/vec4 v0x64078952ee20_0;
    %cmpi/e 0, 0, 1;
    %flag_or 4, 8;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x64078952eee0_0, 0, 1;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x64078952eee0_0, 0, 1;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x64078951df90;
T_2 ;
