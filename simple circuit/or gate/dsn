#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1a341c0 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x1a67500_0 .var "myData", 1 0;
v0x1a675c0_0 .net "out", 0 0, v0x1a67460_0; 1 drivers
L_0x1a67670 .part v0x1a67500_0, 1, 1;
L_0x1a67770 .part v0x1a67500_0, 0, 1;
S_0x1a58510 .scope module, "ourOr" "OR" 2 3, 3 1, S_0x1a341c0;
 .timescale 0 0;
v0x1a34560_0 .net "a", 0 0, L_0x1a67670; 1 drivers
v0x1a673c0_0 .net "b", 0 0, L_0x1a67770; 1 drivers
v0x1a67460_0 .var "c", 0 0;
E_0x1a343f0 .event edge, v0x1a673c0_0, v0x1a34560_0;
    .scope S_0x1a58510;
T_0 ;
    %wait E_0x1a343f0;
    %load/v 8, v0x1a34560_0, 1;
    %load/v 9, v0x1a673c0_0, 1;
    %or 8, 9, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1a67460_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1a341c0;
T_1 ;
    %ix/load 0, 2, 0;
    %assign/v0 v0x1a67500_0, 0, 0;
    %vpi_call 2 8 "$dumpfile", "wave.vcd";
    %vpi_call 2 9 "$dumpvars", 1'sb0, S_0x1a341c0;
    %delay 500, 0;
    %vpi_call 2 10 "$finish";
    %end;
    .thread T_1;
    .scope S_0x1a341c0;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x1a67500_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %ix/load 0, 2, 0;
    %assign/v0 v0x1a67500_0, 0, 8;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "OR.v";
