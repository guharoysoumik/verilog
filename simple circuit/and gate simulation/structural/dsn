#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x202cd90 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
v0x2061130_0 .net "c", 0 0, v0x2061090_0; 1 drivers
v0x2061200_0 .var "myData", 1 0;
L_0x2061280 .part v0x2061200_0, 1, 1;
L_0x2061380 .part v0x2061200_0, 0, 1;
S_0x202ca90 .scope module, "a" "myAnd" 2 6, 3 3, S_0x202cd90;
 .timescale 0 0;
v0x20509c0_0 .net "a", 0 0, L_0x2061280; 1 drivers
v0x2060ff0_0 .net "b", 0 0, L_0x2061380; 1 drivers
v0x2061090_0 .var "c", 0 0;
E_0x202ccc0 .event edge, v0x2060ff0_0, v0x20509c0_0;
    .scope S_0x202ca90;
T_0 ;
    %wait E_0x202ccc0;
    %load/v 8, v0x20509c0_0, 1;
    %load/v 9, v0x2060ff0_0, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v0x2061090_0, 1, 1;
    %vpi_call 3 14 "$monitor", "I am at if part";
    %jmp T_0.1;
T_0.0 ;
    %set/v v0x2061090_0, 0, 1;
T_0.1 ;
    %vpi_call 3 19 "$monitor", "I am at else part";
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x202cd90;
T_1 ;
    %set/v v0x2061200_0, 0, 2;
    %vpi_call 2 9 "$dumpfile", "wave.vcd";
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_0x202cd90;
    %delay 500, 0;
    %vpi_call 2 11 "$finish";
    %end;
    .thread T_1;
    .scope S_0x202cd90;
T_2 ;
    %delay 1, 0;
    %load/v 8, v0x2061200_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %ix/load 0, 2, 0;
    %assign/v0 v0x2061200_0, 0, 8;
    %jmp T_2;
    .thread T_2;
    .scope S_0x202cd90;
T_3 ;
    %vpi_call 2 16 "$monitor", "\012myData[1]: %b\011myData[0]: %b\011c=%b", &PV<v0x2061200_0, 1, 1>, &PV<v0x2061200_0, 0, 1>, v0x2061130_0;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "myAnd.v";
